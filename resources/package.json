    "name": "sws",
    "developer": "Intellivoid",
    "version": "1.0.0.0"
  },
  "build": {
    "type": "alpha",
    "flag": 1
  },
  "installation": {
    "upgradeable": true,
    "default_path_unix": "/usr/local/bin",
